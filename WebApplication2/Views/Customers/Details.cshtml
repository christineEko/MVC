@model WebApplication2.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if (Model.BirthDate > new DateTime(1990, 01,01))
    {
        <li>Birth Date: @Model.BirthDate</li>
    }

</ul>




